{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "chrome",
      "port": 9222,
      "url": "http://localhost:8000*",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/*"
      },
      "sourceMaps": true
    }
  ]
}
